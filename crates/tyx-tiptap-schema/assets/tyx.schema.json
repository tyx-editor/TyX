{
  "topNode": "doc",
  "nodes": {
    "paragraph": {
      "content": "inline*",
      "group": "block",
      "attrs": { "textAlign": { "default": "" }, "dir": { "default": null } },
      "parseDOM": [{ "tag": "p" }]
    },
    "blockquote": {
      "content": "block+",
      "group": "block",
      "defining": true,
      "parseDOM": [{ "tag": "blockquote" }]
    },
    "bulletList": {
      "content": "listItem+",
      "group": "block list",
      "parseDOM": [{ "tag": "ul" }]
    },
    "codeBlock": {
      "content": "text*",
      "marks": "",
      "group": "block",
      "code": true,
      "defining": true,
      "attrs": { "language": { "default": null } },
      "parseDOM": [{ "tag": "pre", "preserveWhitespace": "full" }]
    },
    "doc": { "content": "block+" },
    "hardBreak": {
      "group": "inline",
      "inline": true,
      "selectable": false,
      "linebreakReplacement": true,
      "parseDOM": [{ "tag": "br" }]
    },
    "heading": {
      "content": "inline*",
      "group": "block",
      "defining": true,
      "attrs": {
        "textAlign": { "default": "" },
        "dir": { "default": null },
        "level": { "default": 1 }
      },
      "parseDOM": [
        { "tag": "h1", "attrs": { "level": 1 } },
        { "tag": "h2", "attrs": { "level": 2 } },
        { "tag": "h3", "attrs": { "level": 3 } },
        { "tag": "h4", "attrs": { "level": 4 } },
        { "tag": "h5", "attrs": { "level": 5 } },
        { "tag": "h6", "attrs": { "level": 6 } }
      ]
    },
    "horizontalRule": { "group": "block", "parseDOM": [{ "tag": "hr" }] },
    "orderedList": {
      "content": "listItem+",
      "group": "block list",
      "attrs": { "start": { "default": 1 }, "type": { "default": null } },
      "parseDOM": [{ "tag": "ol" }]
    },
    "text": { "group": "inline" },
    "table": {
      "tableRole": "table",
      "content": "tableRow+",
      "group": "block",
      "isolating": true,
      "parseDOM": [{ "tag": "table" }]
    },
    "tableRow": {
      "tableRole": "row",
      "content": "(tableCell | tableHeader)*",
      "parseDOM": [{ "tag": "tr" }]
    },
    "tableCell": {
      "tableRole": "cell",
      "content": "block+",
      "isolating": true,
      "attrs": {
        "colspan": { "default": 1 },
        "rowspan": { "default": 1 },
        "colwidth": { "default": null }
      },
      "parseDOM": [{ "tag": "td" }]
    },
    "tableHeader": {
      "tableRole": "header_cell",
      "content": "block+",
      "isolating": true,
      "attrs": {
        "colspan": { "default": 1 },
        "rowspan": { "default": 1 },
        "colwidth": { "default": null }
      },
      "parseDOM": [{ "tag": "th" }]
    },
    "image": {
      "group": "block",
      "inline": false,
      "draggable": true,
      "attrs": {
        "src": { "default": null },
        "alt": { "default": null },
        "title": { "default": null }
      },
      "parseDOM": [{ "tag": "img[src]:not([src^=\"data:\"])" }]
    },
    "mathBlock": {
      "group": "block",
      "defining": true,
      "attrs": { "value": { "default": "" }, "asciimath": { "default": "" } },
      "parseDOM": [{ "tag": "math-block" }]
    },
    "mathInline": {
      "group": "inline",
      "inline": true,
      "defining": true,
      "attrs": { "value": { "default": "" }, "asciimath": { "default": "" } },
      "parseDOM": [{ "tag": "math-inline" }]
    },
    "listItem": {
      "content": "paragraph block*",
      "defining": true,
      "parseDOM": [{ "tag": "li" }]
    }
  },
  "marks": {
    "link": {
      "inclusive": true,
      "attrs": {
        "href": { "default": null },
        "target": { "default": "_blank" },
        "rel": { "default": "noopener noreferrer nofollow" },
        "class": { "default": null }
      },
      "parseDOM": [{ "tag": "a[href]" }]
    },
    "textStyle": {
      "attrs": { "color": { "default": null } },
      "parseDOM": [{ "tag": "span" }]
    },
    "bold": {
      "parseDOM": [
        { "tag": "strong" },
        { "tag": "b" },
        { "style": "font-weight=400" },
        { "style": "font-weight" }
      ]
    },
    "code": { "excludes": "_", "code": true, "parseDOM": [{ "tag": "code" }] },
    "italic": {
      "parseDOM": [
        { "tag": "em" },
        { "tag": "i" },
        { "style": "font-style=normal" },
        { "style": "font-style=italic" }
      ]
    },
    "strike": {
      "parseDOM": [
        { "tag": "s" },
        { "tag": "del" },
        { "tag": "strike" },
        { "style": "text-decoration", "consuming": false }
      ]
    },
    "superscript": {
      "parseDOM": [{ "tag": "sup" }, { "style": "vertical-align" }]
    },
    "subscript": {
      "parseDOM": [{ "tag": "sub" }, { "style": "vertical-align" }]
    },
    "underline": {
      "parseDOM": [
        { "tag": "u" },
        { "style": "text-decoration", "consuming": false }
      ]
    },
    "highlight": { "parseDOM": [{ "tag": "mark" }] },
    "typstCode": {
      "excludes": "_",
      "code": true,
      "parseDOM": [
        {
          "tag": "code",
          "attrs": { "data-kind": "typstCode" },
          "preserveWhitespace": "full"
        }
      ]
    }
  }
}
